{
  "name": "@zooniverse/fe-project",
  "description": "Zooniverse front end app handling projects and classification",
  "scripts": {
    "_check": "check-engines && check-dependencies",
    "_serve": "node server",
    "build": "npm run _check && next build",
    "dev": "npm run _check && npm run _serve",
    "start": "npm run _check && NODE_ENV=production npm run _serve",
    "test": "npm run _check && BABEL_ENV=test mocha || true"
  },
  "dependencies": {
    "@zooniverse/grommet-theme": "^2.0.0",
    "check-engines": "^1.5.0",
    "counterpart": "^0.18.6",
    "grommet": "^2.0.0-beta.1",
    "next": "5.1.0",
    "next-routes": "^1.4.1",
    "prop-types": "^15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "styled-components": "^3.2.6"
  },
  "engines": {
    "node": "8"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "check-dependencies": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.8.0",
    "mocha": "^5.1.1"
  }
}
